<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>a966947f-03c9-4511-a135-c601a6a1f391</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
